ENV=dev
LOG_TO_FILE=true

PRIVATE_KEY="your_private_key_here"

# Wallet Configuration
PHANTOM_BASE58_KEY="your_phantom_key_here"
WALLET_ADDRESS="your_wallet_address_here"

# Trading Parameters
INITIAL_INVESTMENT=0.05
MAX_CONCURRENT_TRADES=3
STOP_LOSS_PERCENTAGE=15
TAKE_PROFIT_PERCENTAGE=50
TRADE_INTERVAL=300

# Circuit Breaker
MAX_DAILY_TRADES=10
MAX_DAILY_LOSS=0.01
ERROR_TIMEOUT=300
MAX_ERRORS=3

# Token Validation
MIN_HOLDERS=100
MIN_LIQUIDITY=10
MAX_TOKEN_AGE=600
MIN_GROWTH_POTENTIAL=50

# Risk Management
MAX_POSITION_SIZE=0.05
MAX_SLIPPAGE=0.5
MIN_LIQUIDITY_RATIO=10.0

# API Keys and Endpoints
BIRDEYE_API_KEY="your_birdeye_api_key_here"
SOLSCAN_API_KEY="your_solscan_api_key_here"

# RPC Endpoints
RPC_ENDPOINT="https://api.mainnet-beta.solana.com"
BACKUP_RPC_ENDPOINT="https://solana-api.projectserum.com"

# Telegram Bot
TELEGRAM_BOT_TOKEN="your_telegram_bot_token_here"
TELEGRAM_CHAT_ID="your_telegram_chat_id_here"

# Database
DB_HOST="localhost"
DB_PORT=5432
DB_NAME="ye_meme_trader"
DB_USER="your_db_user"
DB_PASSWORD="your_db_password"

# Monitoring
SENTRY_DSN="your_sentry_dsn_here"

# Trading Settings
TRADE_BUDGET=0.05
MIN_LIQUIDITY_USD=50000
MAX_PRICE_IMPACT=2.0
MIN_HOLDERS=100
